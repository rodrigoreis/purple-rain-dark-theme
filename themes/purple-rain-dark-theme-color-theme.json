{
  "name": "Purple Rain",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#e1bfff"
    },
    "interface": {
      "foreground": "#e1bfff"
    },
    "class": {
      "foreground": "#c191ff"
    },
    "type": {
      "foreground": "#39cc8f"
    }
  },
  "tokenColors": [
    {
      "name": "all other texts",
      "scope": "source",
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    {
      "name": "undefined",
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#ed94c0"
      }
    },
    {
      "name": "return, await, etc",
      "scope": [
        "keyword.control.flow",
        "variable.language.this",
        "keyword.operator.expression.void",
        "keyword.operator.new",
        "storage.modifier.async",
        "keyword.operator.optional",
        "punctuation.accessor.optional",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#875cb8"
      }
    },
    {
      "name": "functions and entities",
      "scope": "entity.",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "support.type.primitive",
        "entity.name.type.module",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    {
      "name": "functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#39cc56"
      }
    },
    {
      "name": "punctuation for template strings",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#c191ff"
      }
    },
    {
      "name": "string templates (verbatim)",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#c9766d"
      }
    },
    {
      "name": "single quote strings",
      "scope": ["string.quoted.single", "punctuation.definition.string"],
      "settings": {
        "foreground": "#c9a26d"
      }
    },
    {
      "name": "comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#435850"
      }
    },
    {
      "name": "numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ff5864"
      }
    },
    {
      "name": "numbers",
      "scope": "constant.numeric.decimal",
      "settings": {
        "foreground": "#ed94c0"
      }
    },
    {
      "name": "properties",
      "scope": ["variable.object.property", "variable.other.property"],
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "name": "constants",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#66c3cc",
        "fontStyle": ""
      }
    },
    {
      "name": "mutable variables",
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#bdbdbd",
        "fontStyle": "underline"
      }
    },
    {
      "name": "variables imports (alias)",
      "scope": "variable.other.readwrite.alias",
      "settings": {
        "foreground": "#c191ff",
        "fontStyle": ""
      }
    },
    {
      "name": "variables imports (alias)",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#bdbdbd",
        "fontStyle": ""
      }
    },
    {
      "name": "variables imports (alias)",
      "scope": ["variable.other.object"],
      "settings": {
        "foreground": "#c191ff"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "meta.brace.round",
        "punctuation",
        "meta.return.type.arrow",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#875cb8"
      }
    },
    {
      "name": "punctuation definition block",
      "scope": ["punctuation.definition.block"],
      "settings": {
        "foreground": "#c191ff"
      }
    },
    {
      "name": "operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#6c95eb"
      }
    }
  ],
