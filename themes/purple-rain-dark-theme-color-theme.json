  "tokenColors": [
    {
      "name": "source",
      "scope": [
        "source",
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "name": "method",
      "scope": [
        "entity.name.function",
      ],
      "settings": {
        "foreground": "#39CC56"
      }
    },
    {
      "name": "number",
      "scope": [
        "constant.numeric",
      ],
      "settings": {
        "foreground": "#EA85D9"
      }
    },
    {
      "name": "strings",
      "scope": [
        "punctuation.definition.string",
        "string.quoted.double",
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#FFD49E"
      }
    },
    {
      "name": "strings template",
      "scope": [
        "punctuation.definition.string.template",
        "string.template",
      ],
      "settings": {
        "foreground": "#C7A370"
      }
    },
    {
      "name": "string regexp pattern",
      "scope": [
        "keyword.control.anchor.regexp",
        "punctuation.definition.group.no-capture.regexp",
        "punctuation.definition.group.regexp",
        "punctuation.definition.character-class.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation",
        "keyword.operator.type.annotation",
        "storage.type.function.arrow",
        "meta.brace.round",
        "meta.brace.square",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.expression",
        "keyword.operator.optional",
        "keyword.operator.type",
        "keyword.operator.ternary.tsx",
        "keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#675875"
      }
    },
    {
      "name": "line comments",
      "scope": [
        "punctuation.definition.comment",
        "comment.line",
      ],
      "settings": {
        "foreground": "#435850"
      }
    },
    {
      "name": "block comments",
      "scope": [
        "comment.block",
      ],
      "settings": {
        "foreground": "#435850"
      }
    },
    {
      "name": "block comments jsdoc parameter",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "storage.type.class.jsdoc",
      ],
      "settings": {
        "foreground": "#164532",
        "fontStyle": "bold"
      }
    },
    {
      "name": "block comments jsdoc parameter value",
      "scope": [
        "variable.other.jsdoc",
      ],
      "settings": {
        "foreground": "#6D593A"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "storage.modifier",
        "storage.type",
        "keyword.control",
        "support.type",
        "variable.language.this",
        "keyword.operator.new",
        "keyword.operator.expression.typeof",
        "keyword.other.class",
        "keyword.type",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "name": "namespace declaration",
      "scope": [
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "name": "class declaration",
      "scope": [
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "name": "enum declaration",
      "scope": [
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#9876AA"
      }
    },
    {
      "name": "enum member",
      "scope": [
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#E1BFFF"
      }
    },
    {
      "name": "type (alias)",
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#E1BFFF"
      }
    },
    {
      "name": "interface declaration",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#875CB8"
      }
    },
    {
      "name": "properties",
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#66C3CC"
      }
    },
    {
      "name": "return, await, etc",
      "scope": [
        "keyword.control.flow",
        "keyword.operator.expression.void",
        "keyword.operator.new",
        "storage.modifier.async",
        "keyword.operator.optional",
        "punctuation.accessor.optional",
        "constant.language"
      ],
      "settings": {
        "foreground": "#875cb8"
      }
    },
    {
      "name": "constants",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "name": "constant escapes",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#D688D4"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "name": "alias",
      "scope": [
        "variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "name": "variable local",
      "scope": [
        "variable.local.readonly"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "name": "parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "builtin types",
      "scope": [
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    {
      "name": "Imported default",
      "scope": [
        "variable.other.constant.object",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name.tsx",
      ],
      "settings": {
        "foreground": "#91B0EC"
      }
    },
    {
      "name": "YAML Entity name",
      "scope": [
        "entity.name.tag.yaml",
      ],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "name": "YAML Alias",
      "scope": [
        "variable.other.alias.yaml"
      ],
      "settings": {
        "foreground": "#E1BFFF"
      }
    },    
    {
      "name": "YAML Entity Name Type Anchor",
      "scope": [
        "entity.name.type.anchor.yaml",
      ],
      "settings": {
        "foreground": "#39CC56"
      }
    },    
    {
      "name": "YAML Plain",
      "scope": [
        "string.unquoted.plain.out.yaml",
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "name": "YAML String",
      "scope": [
        "string.unquoted.plain.in.yaml",
        "string.quoted.single.yaml",
        "string.quoted.double.yaml"
      ],
      "settings": {
        "foreground": "#FFD49E"
      }
    },
    {
      "name": "YAML Punctuation Definition Block",
      "scope": [
        "constant.language.merge.yaml",
        "punctuation.definition.block.sequence.item.yaml",
        "punctuation.definition.anchor.yaml",
        "punctuation.definition.alias.yaml"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    }     
  ],
  "semanticTokenColors": {
    "class.defaultLibrary": {
      "foreground": "#ED94C0"
    },
    "variable.defaultLibrary": {
      "foreground": "#ED94C0"
    },
    "method.static": {
      "foreground": "#39CC8F"
    },
    "method.declaration.static": {
      "foreground": "#39CC8F"
    },
    "parameter.declaration": {
      "foreground": "#FFFFFF"
    },
    "property.declaration.readonly": {
      "foreground": "#66C3CC",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#66C3CC",
      "fontStyle": "italic"
    },
    "variable": {
      "foreground": "#ED94C0"
    },
    "variable.local": {
      "foreground": "#BDBDBD"
    },
    "variable.declaration.local": {
      "foreground": "#BDBDBD"
    },
    "variable.readonly": {
      "foreground": "#C191FF"
    },
    "variable.readonly.local": {
      "foreground": "#BDBDBD"
    },
    "function.readonly.local": {
      "fontStyle": "italic"
    }
  }
}